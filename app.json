{
  "expo": {
    "name": "Math_Master",
    "slug": "Math_Master",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_1024x1024.png",
    "scheme": "mathmaster",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive_foreground.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.juno1.Math_Master"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon-16.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash_1242x2436.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "brand": "mathmaster",
      "router": {},
      "eas": {
        "projectId": "a0d0bb70-b3de-4b24-9a16-ccb698ef9e8e"
      }
    }
  }
}
